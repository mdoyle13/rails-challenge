<h1>Add new expert</h1>
<%= form_for @expert do |f| %>
    <%= render "shared/error_messages", :resource => @expert %>
	<div class="form-group">
        <div><%= f.label :name %></div>
		<div><%= f.text_field :name %></div>
	</div>
    <div class="form-group">
        <div><%= f.label :url %></div>
        <div><%= f.text_field :url %></div>
    </div>
	<div class="form-group">
        <%= f.submit %>
	</div>
<% end %>

